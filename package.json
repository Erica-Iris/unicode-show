{
  "name": "unicode-show",
  "displayName": "unicode-show",
  "description": "Show Unicode Character",
  "author": "1risW",
  "icon": "./images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Erica-Iris/unicode-show.git"
  },
  "publisher": "1risW",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unicode-show.showUnicode",
        "title": "Show Unicode Charater"
      }
    ],
    "keybindings": [
      {
        "command": "unicode-show.showUnicode",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u",
        "when": "editorTextFocus && !editorReadonly"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "license": "MIT",
  "dependencies": {
    "vsce": "^2.15.0"
  }
}